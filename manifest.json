{
  "name": "Randomer",
  "description" : "username generator based on reddit signup page suggestions",
  "version": "1.0.1",
  "browser_action": {
    "default_popup": "Main.html",
    "default_icon": "img/logo.png"
  },

  "icons": { "16": "img/logo.png",
           "48": "img/logo.png",
          "128": "img/logo.png"
        },
  "permissions": [
],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}
